<h1>Change leader</h1>
<%= form_with(model: @project, :url => { action: update_leader_url, :id => @project_id }) do |f| %>
<p>Name: <%= @current_leader.name %></p>
<p>Email: <%= @current_leader.email %></p>
<%= f.label :user_id %>
<%= f.collection_select(:user_id, @users, :user_id, User.find(:user_id).name, prompt: true) %>
<%= f.submit 'Change', class: "btn btn-primary" %>
<% end %>